{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/constants/*": ["./src/constants/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/helpers/*": ["./src/helpers/*"],
      "@/styles": ["./src/styles"],
      "@/utils/*": ["./src/utils/*"]
    },
    "module": "commonjs"
  },
  "exclude": [
    "node_modules"
  ]
}